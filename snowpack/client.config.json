{
  "install": [
    "svelte"
  ],
  "plugins": [
    ["../snowpack/plugins/plugin-svelte"]
  ],
  "devOptions": {
    "port": 3002,
    "out": "build/client",
    "open": "none"
  },
  "mount": {
    "routes": "/_routes/",
    "sapper": "/_sapper/",
    "shared": "/shared/",
    "public": "/"
  },
  "alias": {
    "@shared": "./shared"
  }
}
